{
  "name": "yannova-website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "analyze": "ANALYZE=true npm run build",
    "build:prod": "NODE_ENV=production npm run build",
    "type-check": "tsc --noEmit",
    "preview": "npm run build && npm run start",
    "test": "echo \"No tests specified\" && exit 0",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "clean": "rm -rf .next out",
    "docker:build": "docker build -t yannova-website .",
    "docker:run": "docker run -p 3000:3000 yannova-website",
    "docker:up": "docker-compose up --build",
    "docker:down": "docker-compose down",
    "deploy:railway": "./deploy-to-railway.sh",
    "deploy:render": "./deploy-to-render.sh",
    "test:docker": "./test-docker.sh",
    "prepare": "husky"
  },
  "dependencies": {
    "@google/generative-ai": "^0.21.0",
    "bcryptjs": "^2.4.3",
    "critters": "^0.0.23",
    "lucide-react": "^0.453.0",
    "next": "14.2.18",
    "next-auth": "^4.24.10",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "resend": "^6.1.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^15.5.6",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "autoprefixer": "^10.4.20",
    "eslint": "^8",
    "eslint-config-next": "14.2.18",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.6",
    "postcss": "^8",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
